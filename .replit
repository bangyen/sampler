modules = ["python-3.11", "postgresql-16"]

[nix]
channel = "stable-25_05"
packages = ["cmake", "gcc", "gnumake", "hdf5", "just", "libjpeg_turbo", "libpng", "libxcrypt", "which"]

[deployment]
deploymentTarget = "autoscale"
run = ["streamlit", "run", "app.py", "--server.port", "5000"]

[workflows]
runButton = "Project"

[[workflows.workflow]]
name = "Project"
mode = "parallel"
author = "agent"

[[workflows.workflow.tasks]]
task = "workflow.run"
args = "fastapi-app"

[[workflows.workflow]]
name = "fastapi-app"
author = "agent"

[workflows.workflow.metadata]
outputType = "webview"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "uvicorn server:app --host 0.0.0.0 --port 5000"
waitForPort = 5000

