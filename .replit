modules = ["python-3.11", "postgresql-16"]

[nix]
channel = "stable-25_05"

[deployment]
deploymentTarget = "autoscale"
run = ["streamlit", "run", "app.py", "--server.port", "5000"]

[workflows]
runButton = "Project"

[[workflows.workflow]]
name = "Project"
mode = "parallel"
author = "agent"

[[workflows.workflow.tasks]]
task = "workflow.run"
args = "fastapi-app"

[[workflows.workflow]]
name = "fastapi-app"
author = "agent"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "python server.py"
waitForPort = 5000

[workflows.workflow.metadata]
outputType = "webview"

[[ports]]
localPort = 5000
externalPort = 80

[[ports]]
localPort = 33913
externalPort = 4200

[[ports]]
localPort = 35941
externalPort = 5000

[[ports]]
localPort = 36335
externalPort = 3002

[[ports]]
localPort = 38311
externalPort = 3000

[[ports]]
localPort = 39795
externalPort = 3003

[[ports]]
localPort = 42087
externalPort = 5173

[[ports]]
localPort = 42435
externalPort = 3001

[agent]
integrations = ["streamlit_database:1.0.0"]
